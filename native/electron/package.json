{
  "name": "mcbbs-client",
  "version": "0.2.0",
  "description": "A BBS client for MCBBS.",
  "main": "index.js",
  "scripts": {
    "start": "electron ./main.js",
    "watch": "watchify dialogRender.js -t babelify -o public/js/bundle.js --debug --verbose",
    "watchTest": "watchify ./scripts/viewManager/utils/_headImage.js -t babelify -o ./scripts/viewManager/utils/headImage.js --debug --verbose",
    "package": "electron-packager ./ DemoApps --overwrite --app-version=1.0.0 --platform=win32 --arch=all --out=../DemoApps --version=1.2.1 --icon=./public/img/app-icon.icns",
    "test": "jest"
  },
  "author": "langyo",
  "license": "Unlicense",
  "dependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0-beta.0",
    "classnames": "^2.2.6",
    "firebase": "^5.6.0",
    "lowdb": "^1.0.0",
    "mcbbs-client-scripts": "^0.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-swipeable-views": "^0.13.0",
    "react-swipeable-views-utils": "^0.13.0",
    "regenerator-runtime": "^0.13.1",
    "watchify": "^3.11.0",
    "webpack": "^4.26.1"
  },
  "keywords": [],
  "devDependencies": {
    "babelify": "^10.0.0",
    "electron": "^3.0.10",
    "electron-packager": "^12.2.0",
    "jest": "^23.6.0",
    "react-desktop": "^0.3.7"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "current"
          }
        }
      ],
      [
        "@babel/preset-react"
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  }
}
